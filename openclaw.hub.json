{
  "id": "d1-vec-cfbrowser-worker",
  "name": "D1 + Vectorize Memory Worker",
  "type": "bundle",
  "version": "0.1.0",
  "description": "Worker exposing a Memory API backed by Cloudflare D1 + Vectorize",
  "components": {
    "worker": {
      "path": "./",
      "routes": [
        "/memory/store",
        "/memory/recall",
        "/memory/forget"
      ]
    }
  },
  "requirements": {
    "cloudflare": ["workers", "d1", "vectorize"],
    "secrets": ["MEMORY_API_SECRET", "EMBEDDING_API_KEY"]
  }
}
